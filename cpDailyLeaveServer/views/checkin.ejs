  <script>
    function checkedIn() {
      document.getElementById("in1").classList.add("radio-checked");
      document.getElementById("out1").classList.remove("radio-checked");

      document.getElementById("inInput").style.display = "";
      document.getElementById("outInput").style.display = "none";
    }
    function checkedOut() {
      document.getElementById("out1").classList.add("radio-checked");
      document.getElementById("in1").classList.remove("radio-checked");

      document.getElementById("inInput").style.display = "none";
      document.getElementById("outInput").style.display = "";
    }
    function submit() {
      var phone = document.getElementById("phone").innerText;
      var name = document.getElementById("name").innerText;
      var identity = document.getElementById("identity").innerText;

      location.href = "./checksuccess?phone=" + phone+"&name="+name+"&identity="+identity

    }
  </script>
